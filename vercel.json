{
  "builds": [
    {
      "src": "BackEnd-ecommerce/Index.js",
      "use": "@vercel/node"
    },
  {
      "src": "BackEnd-ecommerce/build/**",
      "use": "@vercel/static"
    }
  ],
 "routes":[
    {
      "src": "/api/(.*)",
      "dest": "BackEnd-ecommerce/Index.js"
    },
    {
      "src": "/",
      "dest": "BackEnd-ecommerce/build/index.html"
    },
    {
      "src": "/api/auth",
      "dest": "BackEnd-ecommerce/build/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "BackEnd-ecommerce/build/$1"
    }
  ]
}
